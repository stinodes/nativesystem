{
  "name": "nativesystem",
  "version": "1.0.0-beta.4",
  "author": "stinodes",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "flow": "flow",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "test-ci": "jest",
    "storybook": "storybook start -p 7007",
    "prepack": "yarn prepack:babel && yarn prepack:flow",
    "prepack:babel": "babel src/ -d lib --ignore '**/*.test.js'",
    "prepack:flow": "flow-copy-source src/ lib/",
    "prettier": "prettier --jsx-bracket-same-line true --single-quote --trailing-comma all --write \"./src/**\""
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "dependencies": {
    "fnional": "^0.0.5",
    "glamorous-native": "^1.4.0",
    "polished": "^1.9.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.0-alpha.1",
    "@storybook/addon-knobs": "^3.4.0-alpha.1",
    "@storybook/addon-links": "^3.4.0-alpha.1",
    "@storybook/react-native": "^3.4.0-alpha.1",
    "babel-cli": "^6.26.0",
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "4.0.0",
    "flow-bin": "0.75",
    "flow-copy-source": "^2.0.1",
    "formik": "^1.3.0",
    "jest": "^23.4.0",
    "metro-react-native-babel-preset": "^0.45.3",
    "prettier": "^1.14.2",
    "react": "16.5.0",
    "react-dom": "^16.3.2",
    "react-native": "0.57.0",
    "react-native-vector-icons": "^4.6.0",
    "react-test-renderer": "16.3.1"
  },
  "peerDependencies": {
    "react": "16.5.0",
    "react-dom": "^16.3.2",
    "react-native": "0.57.0",
    "react-native-vector-icons": "4.x"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/?!(fnional)"
    ],
    "testPathIgnorePatterns": [
      "./__tests__/.*.mocks.js"
    ]
  }
}
